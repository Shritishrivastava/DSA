#include <bits/stdc++.h> 
vector<pair<char,int>> duplicate_char(string S, int n){

   unordered_map<char, int> freq;
   vector<pair<char, int>>ans;
   for(int i = 0; i < n; i++){
       freq[S[i]]++;
   }

   for(auto &item : freq){
       if(item.second > 1){
           ans.push_back({item.first, item.second});
       }
   }
    sort(ans.begin(), ans.end());
    return ans;
}
