Node* reverseDLL(Node* head)
{   
    if(head == nullptr || head->next == nullptr){
        return head;
    }

    Node* curr = head;
    Node* temp = nullptr;
    while(curr!=nullptr){
        temp = curr -> prev;
        curr->prev = curr->next;
        curr->next = temp;
        curr = curr->prev;
    }
    return temp->prev;
}
